<plugin plugin="products" group="framework" version="1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schema.qcadoo.org/plugin"
	xmlns:model="http://schema.qcadoo.org/modules/model"
	xsi:schemaLocation="
		http://schema.qcadoo.org/plugin 
		http://schema.qcadoo.org/plugin.xsd 
		http://schema.qcadoo.org/modules/model 
		http://schema.qcadoo.org/modules/model.xsd">

	<information>
		<name>products</name>
	</information>

	<modules>
		<model:model model="product" resource="model/product.xml" />
		<model:model model="productListDto" resource="model/productListDto.xml" />
		<model:model model="component" resource="model/component.xml" />
		<model:model model="part" resource="model/part.xml" />
		
		<model:model model="factory" resource="model/factory.xml" />
		<model:model model="division" resource="model/division.xml" />

		<model:model model="versionableEntity" resource="model/versionableEntity.xml" />
	</modules>

</plugin>